    "chai": "^4.3.9",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "mocha": "^10.2.0",
    "rimraf": "~5.0.0",
    "ts-mocha": "^10.0.0",
    "typescript": "~4.9.0",
    "zod": "^3.22.2"
  },
  "dependencies": {
